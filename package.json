{
  "name": "cucumber-protractor-demo",
  "version": "0.0.3",
  "scripts": {
    "postinstall": "npm run hooks:setup && ./node_modules/.bin/webdriver-manager update",
    "lint": "tslint -p ./",
    "e2e": "npm run e2e:chrome",
    "pree2e:chrome": "npm run reports:clean",
    "e2e:chrome": "protractor e2e/protractor.conf.js",
    "pree2e:firefox": "npm run reports:clean",
    "e2e:firefox": "protractor e2e/protractor.firefox.conf.js",
    "hooks:setup": "shx mkdir -p .git/hooks && shx cp hooks/* .git/hooks",
    "reports:clean": "npm run reports:clean:json && npm run reports:clean:html",
    "reports:clean:json": "shx rm -f e2e/test-reports/json/*.json && shx rm -rf e2e/test-reports/json/json-output-folder",
    "reports:clean:html": "shx rm -rf e2e/test-reports/html/*"
  },
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/cucumber": "^4.0.7",
    "@types/node": "~8.9.4",
    "@types/selenium-webdriver": "3.0.15",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codelyzer": "^5.2.2",
    "cucumber": "^6.0.5",
    "protractor": "^7.0.0",
    "protractor-cucumber-framework": "^6.2.1",
    "protractor-multiple-cucumber-html-reporter-plugin": "^1.8.1",
    "rxjs": "^6.6.3",
    "shx": "^0.3.3",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}
